{
  "name": "dunglas/phpdoc-to-typehint",
  "license": "MIT",
  "type": "project",
  "description": "Generate scalar type hints from PHPDoc annotations",
  "autoload": {
    "psr-4": { "Dunglas\\PhpDocToTypeHint\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Dunglas\\PhpDocToTypeHint\\Tests\\": "tests/" }
  },
  "require": {
    "php": ">=7.0.0",
    "ext-tokenizer": "*",
    "symfony/console": "^2.7.0",
    "symfony/finder": "^2.7.0",
    "phpdocumentor/reflection": "^4.0@dev",
    "phpdocumentor/reflection-common": "^1.0@dev",
    "phpdocumentor/reflection-docblock": "^5.0dev",
    "sebastian/diff": "^1.3",
    "nikic/php-parser": "^4.0@dev"
  },
  "bin": ["bin/phpdoc-to-typehint"]
}
